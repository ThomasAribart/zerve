{
  "name": "z-server",
  "private": true,
  "version": "0.0.0",
  "types": "index.ts",
  "main": "index.ts",
  "scripts": {
    "start": "node ./build/index.js",
    "start:watch": "nodemon ./build/index.js",
    "build": "esbuild ./index.js --bundle --platform=node --outfile=build/index.js",
    "build:watch": "yarn build --watch",
    "dev": "concurrently \"yarn start:watch\" \"yarn build:watch\"",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "yarn lint:fix"
  },
  "dependencies": {
    "@zerve/core": "*",
    "@zerve/node": "*",
    "ajv": "^8.10.0",
    "body-parser": "^1.19.1",
    "crypto": "*",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "json-schema-to-ts": "^1.6.4",
    "next-compose-plugins": "^2.2.1",
    "obs-websocket-js": "^4.0.3",
    "react-native-web": "^0.17.5",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.10",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.13",
    "eslint": "^8.7.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.3"
  }
}
